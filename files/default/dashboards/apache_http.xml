<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>http</label>
  <row>
    <single>
      <searchString>source="/var/log/apache2/access.log" | stats count</searchString>
      <title>Requests per minute</title>
      <earliestTime>rt-1m</earliestTime>
      <latestTime>rt</latestTime>
    </single>
  </row>
  <row>
    <chart>
      <searchString>source="/var/log/apache2/access.log" | timechart count</searchString>
      <title>RealTime Requests</title>
      <earliestTime>rt-5m</earliestTime>
      <latestTime>rt</latestTime>
      <option name="charting.chart">area</option>
      <option name="drilldown">all</option>
    </chart>
  </row>
  <row>
    <chart>
      <searchString>source="/var/log/apache2/access.log" (500 OR 501 OR 502 OR 503 OR 504) | timechart count</searchString>
      <title>50X errors</title>
      <earliestTime>-1d</earliestTime>
      <latestTime>now</latestTime>
      <option name="charting.chart">area</option>
      <option name="drilldown">all</option>
    </chart>
    <chart>
      <searchString>source="/var/log/apache2/access.log" (400 OR 401 OR 403 OR 404) | timechart count</searchString>
      <title>40X errors</title>
      <earliestTime>-1d</earliestTime>
      <latestTime>now</latestTime>
      <option name="charting.chart">area</option>
      <option name="drilldown">all</option>
    </chart>
  </row>
  <row>
    <chart>
      <searchString>source="/var/log/apache2/access.log" 200 | timechart count</searchString>
      <title>200</title>
      <earliestTime>-1d</earliestTime>
      <latestTime>now</latestTime>
      <option name="charting.chart">area</option>
      <option name="drilldown">all</option>
    </chart>
  </row>
</dashboard>